<?xml version="1.0" encoding="UTF-8"?>
<web-app>
  <display-name>Servlet with Mysql connection</display-name>
  <servlet>
  	<servlet-name>Registration</servlet-name>
  	<servlet-class>RegisterServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Registration</servlet-name>
  	<url-pattern>/register.php</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>Deletion</servlet-name>
  	<servlet-class>DeleteServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Deletion</servlet-name>
  	<url-pattern>/delete.php</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>Login</servlet-name>
  	<servlet-class>LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Login</servlet-name>
  	<url-pattern>/login.php</url-pattern>
  </servlet-mapping>
</web-app>